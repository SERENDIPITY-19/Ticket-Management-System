                            <div class="dropdown severity-dropdown me-2">
                                <span class="me-1">Severity:</span>
                                <button class="btn badge severity-badge severity-minor dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Minor
                                </button>
                                <ul class="dropdown-menu severity-list">
                                    <li><button class="dropdown-item severity-option text-success" type="button" data-severity="minor">Minor</button></li>
                                    <li><button class="dropdown-item severity-option text-warning" type="button" data-severity="moderate">Moderate</button></li>
                                    <li><button class="dropdown-item severity-option text-danger" type="button" data-severity="major">Major</button></li>
                                    <li><button class="dropdown-item severity-option text-danger fw-bold" type="button" data-severity="critical">Critical / ShowStopper</button></li>
                                </ul>
                            </div>

                            <div class="dropdown priority-dropdown">
                                <span class="me-1">Priority:</span>
                                <button class="btn badge priority-badge priority-low dropdown-toggle" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    Low
                                </button>
                                <ul class="dropdown-menu priority-list">
                                    <li><button class="dropdown-item priority-option text-success" type="button" data-priority="low">Low</button></li>
                                    <li><button class="dropdown-item priority-option text-warning" type="button" data-priority="medium">Medium</button></li>
                                    <li><button class="dropdown-item priority-option text-danger" type="button" data-priority="high">High</button></li>
                                    <li><button class="dropdown-item priority-option text-danger fw-bold" type="button" data-priority="critical">Critical / Blocker</button></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="d-flex align-items-center ms-auto gap-2">
                        <span class="badge text-black bg-light border">Ticket #22</span>
                        <div class="dropdown">
                            <button class="btn bg-light border" id="StatusDropdown" title="Ticket Status" data-bs-toggle="dropdown" aria-expanded="false">
                                <i class="fa-regular fa-circle ticket-open"></i>
                            </button>
                            <ul class="dropdown-menu" id="statusList">
                                <li><button class="dropdown-item" type="button"><i class="fa-regular fa-circle ticket-open"></i> Open</button></li>
                                <li><button class="dropdown-item" type="button"><i class="fa-solid fa-clock ticket-in-progress"></i> In Progress</button></li>
                                <li><button class="dropdown-item" type="button"><i class="fa-solid fa-pause ticket-on-hold"></i> On Hold</button></li>
                                <li><button class="dropdown-item" type="button"><i class="fa-solid fa-hourglass-half ticket-pending"></i> Pending</button></li>
