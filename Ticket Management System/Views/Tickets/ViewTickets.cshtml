@{
    ViewData["Title"] = "View Tickets";
}
<div class="container my-4">
    <h4 class="mb-3"><span><a class="text-decoration-none" asp-controller="Tickets" asp-action="Index">Ticket</a></span> <span>/ Ticket details</span></h4>
